{
    "name": "tjc-scheduling-app",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "server": "node -r ts-node/register ./server/index.ts",
        "server:watch": "nodemon -r ts-node/register ./server/index.ts",
        "seed": "ts-node scripts/seed.js",
        "seed2": "ts-node scripts/seed2.js",
        "set-mac-env": "chmod +x scripts/set_mac_env.sh && scripts/set_mac_env.sh",
        "prettier:base": "prettier",
        "prettier:check": "npm run prettier:base -- --list-different \"**/*\"",
        "prettier:write": "npm run prettier:base -- --write \"**/*\"",
        "set-win-env": "powershell.exe -exec bypass -File D:/Code Projects/tjc-scheduling-app/scripts/set_win_env.ps1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*": [
            "npm run prettier:base -- --write"
        ]
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "server"
        ],
        "exec": "npm run server",
        "ext": "ts"
    },
    "dependencies": {
        "@babel/node": "^7.8.7",
        "@expo/vector-icons": "^10.0.6",
        "@react-native-community/async-storage": "^1.10.0",
        "@react-native-community/masked-view": "0.1.5",
        "@react-navigation/bottom-tabs": "^5.2.5",
        "@react-navigation/drawer": "^5.4.0",
        "@react-navigation/native": "^5.1.7",
        "@react-navigation/stack": "^5.2.9",
        "@typescript-eslint/parser": "^2.31.0",
        "axios": "^0.19.2",
        "body-parser": "^1.19.0",
        "chalk": "^4.0.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^8.2.0",
        "expo": "~36.0.0",
        "expo-font": "~8.0.0",
        "express": "^4.17.1",
        "morgan": "^1.10.0",
        "pg": "^8.0.2",
        "pg-hstore": "^2.3.3",
        "react": "~16.9.0",
        "react-dom": "~16.9.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
        "react-native-gesture-handler": "~1.5.0",
        "react-native-keyboard-aware-scroll-view": "^0.9.1",
        "react-native-reanimated": "~1.4.0",
        "react-native-safe-area-context": "0.6.0",
        "react-native-screens": "2.0.0-alpha.12",
        "react-native-web": "~0.11.7",
        "react-navigation": "^4.3.9",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "sequelize": "^4.44.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/register": "^7.9.0",
        "@babel/runtime": "^7.9.2",
        "@types/express": "^4.17.6",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.5",
        "@types/react": "^16.9.35",
        "@types/react-native": "^0.62.7",
        "@types/react-test-renderer": "^16.9.2",
        "@types/sequelize": "^4.28.8",
        "@types/validator": "^13.0.0",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "babel-loader": "^8.1.0",
        "babel-preset-expo": "~8.0.0",
        "compression": "^1.7.4",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^7.2.1",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "metro-react-native-babel-preset": "^0.59.0",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.3",
        "path": "^0.12.7",
        "prettier": "^2.0.5",
        "prettier-eslint": "^9.0.1",
        "pretty-quick": "^2.0.1",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    },
    "private": true
}
