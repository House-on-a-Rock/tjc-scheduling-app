{
  "name": "tjc-scheduling-app",
  "scripts": {
    "server": "node -r ./server/index.js",
    "server-watch": "nodemon -r ./server/index.js  -e html,js,jsx,css --ignore dist --ignore client",
    "prettier:base": "prettier",
    "prettier:check": "npm run prettier:base -- --list-different \"**/*\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*\"",
    "set-mac-env": "chmod +x scripts/set_mac_env.sh && scripts/set_mac_env.sh",
    "set-win-env": "powershell.exe -exec bypass -File D:/Code Projects/tjc-scheduling-app/scripts/set_win_env.ps1",
    "test": "mocha -r tests/*.js --reporter spec --exit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "npm run prettier:base -- --write"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js",
      ".git",
      "node_modules"
    ],
    "watch": [
      "server",
      "src"
    ],
    "exec": "npm run server"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "express-jwt-authz": "^2.3.1",
    "express-openid-connect": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "jwt-decode": "^3.1.1",
    "luxon": "^1.24.1",
    "mocha": "^8.2.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.8",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sequelize": "^4.44.4",
    "uninstall": "0.0.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "compression": "^1.7.4",
    "css-loader": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "pretty-quick": "^2.0.1",
    "style-loader": "^2.0.0"
  },
  "private": true
}
