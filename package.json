{
  "name": "tjc-scheduling-app",
  "scripts": {
    "start": "node -r ts-node/register ./server/index.ts  -e html,js,scss --ignore dist --ignore client",
    "heroku-prebuild": "cd src && npm i",
    "build-windows-watch": "webpack -w",
    "build-windows-removeDist": "rd /s /q dist",
    "set-windows-env": "SET NODE_ENV='development'",
    "dev-windows": "npm run set-windows-env & npm run build-windows-removeDist & npm run build-windows-watch & npm run server-watch",
    "dev": "SET NODE_ENV='development' & npm run build-watch & npm run server-watch",
    "build": "webpack --mode production",
    "build-prod": "webpack --mode production",
    "build-watch": "rm -rf dist && webpack -w",
    "server": "node -r ts-node/register ./server/index.ts  -e html,js,scss --ignore dist --ignore client",
    "server-watch": "nodemon -r ts-node/register ./server/index.ts  -e html,js,jsx,ts,tsx,css --ignore dist --ignore client",
    "prettier:base": "prettier",
    "prettier:check": "npm run prettier:base -- --list-different \"**/*\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*\"",
    "seed": "ts-node scripts/seed.js",
    "set-mac-env": "chmod +x scripts/set_mac_env.sh && scripts/set_mac_env.sh",
    "set-win-env": "powershell.exe -exec bypass -File D:/Code Projects/tjc-scheduling-app/scripts/set_win_env.ps1",
    "test": "mocha -r ts-node/register tests/*.ts --reporter spec --exit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "npm run prettier:base -- --write"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "server",
      "src"
    ],
    "exec": "npm run server",
    "ext": "ts"
  },
